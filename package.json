{
  "name": "frontend",
  "version": "1.4.0",
  "description": "",
  "main": "webpack.config.js",
  "scripts": {
    "eslint": "./node_modules/.bin/eslint ./src/index.js",
    "test": "jest --coverage",
    "start": "webpack-dev-server --mode development --env.ENVIRONMENT=development --env.ENV_FILE_PATH=$PWD/env/development.env"
  },
  "author": "",
  "license": "ISC",
  "browserslist": [
    "last 2 versions"
  ],
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./src/setupTests.js",
      "jest-localstorage-mock"
    ],
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "identity-obj-proxy",
      "\\.(png|svg)$": "identity-obj-proxy",
      "\\.(gif|ttf|eot)$": "<rootDir>/__mocks__/fileMock.js",
      "@root/(.*)": "<rootDir>/$1",
      "@actions(.*)": "<rootDir>/src/actions$1",
      "@api(.*)": "<rootDir>/src/api$1",
      "@components(.*)": "<rootDir>/src/components$1",
      "@constants(.*)": "<rootDir>/src/constants$1",
      "@containers(.*)": "<rootDir>/src/containers$1",
      "@pages(.*)": "<rootDir>/src/pages$1",
      "@reducers(.*)": "<rootDir>/src/reducers$1",
      "@styles(.*)": "<rootDir>/src/styles$1",
      "@utils(.*)": "<rootDir>/src/utils$1"
    }
  },
  "dependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "autoprefixer": "^9.7.2",
    "axios": "^0.21.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-react-jsx-img-import": "^0.1.4",
    "history": "^4.10.1",
    "path": "^0.12.7",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.1"
  },
  "devDependencies": {
    "css-loader": "^3.2.0",
    "dotenv-webpack": "^1.7.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "file-loader": "^4.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "img-loader": "^3.0.1",
    "jest": "^24.9.0",
    "jest-localstorage-mock": "^2.4.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.0",
    "url-loader": "^2.2.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  }
}
